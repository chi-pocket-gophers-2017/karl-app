<h1>Users show</h1>

<%= button_to "Make a new request", requests_path %>
<div id="requests-in-progress">
	<h1>In Progress:</h1>
	<% @requests_in_progress.each do |r| %>
	<%= render partial: 'requests/request_in_progress', locals: {r: r} %>
	<% end %>
</div>

<br>
<br>
<br>

<div id="requests">
	<h1>Queue</h1>
<% @pending_requests.each do |r| %>
	<%= render partial: 'requests/pending_request', locals: {r: r} %>
<% end %>
</div>

